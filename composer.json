{
	"name" : "avored/framework",
	"description" : "AvoRed Laravel E commerce Framework",
	"keywords" : [
		"framework",
		"cart",
		"laravel",
		"e commerce",
		"laravel5",
		"shop",
		"shopping-cart",
		"e-commerce",
		"shopping cart",
		"e commerce"
	],
	"license" : "MIT",
	"authors" : [{
			"name" : "Purvesh  ",
			"email" : "ind.purvesh@gmail.com"
		}
	],
	"type" : "library",
	"require" : {
		"php" : ">=7.1.3",
        "illuminate/auth": "^7.0",
        "illuminate/routing": "^7.0",
        "illuminate/support": "^7.0",
        "illuminate/http": "^7.0",
        "illuminate/database": "^7.0",
        "illuminate/container": "^7.0",
        "illuminate/console": "^7.0",
        "illuminate/mail": "^7.0",
        "illuminate/view": "^7.0",
        "illuminate/session": "^7.0",
        "illuminate/validation": "^7.0",
        "illuminate/translation": "^7.0",
        "illuminate/pagination": "^7.0",
        "illuminate/notifications": "^7.0",
        "illuminate/filesystem": "^7.0",
        "illuminate/events": "^7.0",
        "illuminate/encryption": "^7.0",
        "illuminate/contracts": "^7.0",
        "illuminate/config": "^7.0"
    },
	"require-dev" : {
		"phpunit/phpunit" : "~8.1",
        "orchestra/testbench": "^5.1",
		"mockery/mockery": "^1.3"
	},
	"autoload" : {
		"classmap": [
            "database/factories"
        ],
		"psr-4" : {
			"AvoRed\\Framework\\" : "src/"

		}
	},
	"autoload-dev" : {
		"psr-4" : {
			"AvoRed\\Framework\\Tests\\" : "tests/"
		}
	},
	"extra" : {
		"laravel" : {
			"providers" : [
				"AvoRed\\Framework\\AvoRedProvider"
			],
			"aliases" : {
				"Menu" : "AvoRed\\Framework\\Support\\Facades\\Menu",
                "Tab" : "AvoRed\\Framework\\Support\\Facades\\Tab"
			}
		}
	},
	"homepage" : "https://www.avored.com",
	"support" : {
		"email" : "ind.purvesh@gmail.com",
		"issues" : "https://www.avored.com/discussion",
		"forum" : "https://www.avored.com/discussion",
		"wiki" : "https://www.avored.com/docs",
		"source" : "https://www.github.com/avored/framework"
	}
}
